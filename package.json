{
  "name": "@vestfoldfylke/loglady",
  "version": "1.2.0",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "pack": "npm run test && rm -rf dist && rm -f vestfoldfylke-loglady-*.tgz && npm run build && npm pack",
    "test": "biome check && node --import tsx --test src/tests/**/*.test.ts",
    "lint:fix": "biome check --write",
    "lint:fix:unsafe": "biome check --write --unsafe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vestfoldfylke/loglady.git"
  },
  "bugs": {
    "url": "https://github.com/vestfoldfylke/loglady/issues"
  },
  "homepage": "https://github.com/vestfoldfylke/loglady/#readme",
  "keywords": [
    "log",
    "logging",
    "structured-logging"
  ],
  "author": {
    "name": "Rune Moskvil Lyng√•s",
    "email": "rune.moskvil.lyngas@vestfoldfylke.no",
    "url": "https://github.com/runely"
  },
  "license": "MIT",
  "description": "My log has something to tell you",
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.8.1",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-esbuild": "^6.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
